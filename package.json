  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/mypurecloud/webrtc-stats-gatherer/issues"
  },
  "homepage": "https://github.com/mypurecloud/webrtc-stats-gatherer#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "jest": "^26.4.2",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.2",
    "run-sequence": "^2.0.0",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-semistandard": "^8.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.1.0"
  },
