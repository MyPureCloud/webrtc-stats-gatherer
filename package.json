{
  "name": "webrtc-stats-gatherer",
  "version": "1.5.3",
  "description": "Gathers stats on interval for webrtc peer connection",
  "main": "src/StatsGatherer.js",
  "scripts": {
    "lint": "node_modules/.bin/semistandard",
    "build": "node_modules/.bin/browserify src/StatsGatherer.js -s StatsGatherer -o out/stats-gatherer.js -t [ babelify --presets [ es2015 ] ]",
    "prepublish": "npm run build && npm run _test",
    "_test": "node_modules/.bin/mocha --compilers js:babel-core/register test/test.js",
    "test-server": "npm run _test -- -w",
    "test": "npm run lint && npm run _test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mypurecloud/webrtc-stats-gatherer.git"
  },
  "keywords": [
    "webrtc",
    "stats"
  ],
  "author": "Xander Dumaine <xander.dumaine@inin.com>, Garrett Jensen <garrett.jensen@inin.com>, Fippo & Lance <3",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mypurecloud/webrtc-stats-gatherer/issues"
  },
  "homepage": "https://github.com/mypurecloud/webrtc-stats-gatherer#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "semistandard": "^8.0.0",
    "sinon": "^1.17.4"
  },
  "pre-push": [
    "test"
  ],
  "pre-commit": [
    "lint"
  ],
  "semistandard": {
    "ignore": [
      "**/out/",
      "/lib/select2/",
      "/lib/ckeditor/",
      "tmp.js"
    ]
  }
}
