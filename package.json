{
  "name": "webrtc-stats-gatherer",
  "version": "0.1.0",
  "description": "Gathers stats on interval for webrtc peer connection",
  "main": "index.js",
  "scripts": {
    "compile": "node_modules/.bin/babel --presets es2015 -d out/ src/StatsGatherer.js",
    "prepublish": "npm run compile",
    "test": "npm run compile && node_modules/.bin/semistandard && node_modules/.bin/mocha --compilers js:babel-core/register test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mypurecloud/webrtc-stats-gatherer.git"
  },
  "keywords": [
    "webrtc",
    "stats"
  ],
  "author": "Fippo Hancke, Lance Stout, Xander Dumaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mypurecloud/webrtc-stats-gatherer/issues"
  },
  "homepage": "https://github.com/mypurecloud/webrtc-stats-gatherer#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "semistandard": "^8.0.0"
  },
  "semistandard": {
    "ignore": [
      "**/out/",
      "/lib/select2/",
      "/lib/ckeditor/",
      "tmp.js"
    ]
  }
}
